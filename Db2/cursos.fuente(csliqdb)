      *----------------------------------------------------------------+00087499
      *                                                                *00087524
      *  CUERPO INICIO APERTURA ARCHIVOS                               *00087624
      *                                                                *00087724
      *----------------------------------------------------------------+00087899
       1000-I-INICIO.                                                   00087901
                                                                        00088099
           ACCEPT METODO-LIQUIDACION FROM SYSIN.                        00088199
                                                                        00088399
      *    OPEN DE ARCHIVOS CLIENTE / LIQUIDACION / DETALLE LIQ         00088499
           OPEN INPUT MCLIEN.                                           00088599
           IF WS-CLI-CODE IS NOT EQUAL '00'                             00088699
              DISPLAY '* ERROR EN OPEN CLIENTES  = ' WS-CLI-CODE        00088799
              MOVE 'Y' TO WS-EOF                                        00088899
           END-IF.                                                      00088999
                                                                        00089099
           OPEN OUTPUT REPORTE.                                         00090599
           IF WS-REP-CODE IS NOT EQUAL '00'                             00090699
              DISPLAY '* ERROR EN OPEN REPORTE =  ' WS-REP-CODE         00090799
              MOVE 'Y' TO WS-EOF                                        00090899
           END-IF.                                                      00090999
                                                                        00091799
           OPEN OUTPUT LIQUI.                                           00091899
           IF WS-LIQ-CODE IS NOT EQUAL '00'                             00091999
              DISPLAY '* ERROR EN OPEN LIQUI   =  ' WS-LIQ-CODE         00092099
              MOVE 'Y' TO WS-EOF                                        00092199
           END-IF.                                                      00092299
                                                                        00092399
      *    OPEN CURSORES                                                00092499
           EXEC SQL                                                     00092599
              OPEN DESCR                                                00092699
           END-EXEC.                                                    00092799
                                                                        00092899
           IF SQLCODE NOT EQUAL ZEROS                                   00092999
              MOVE SQLCODE   TO WS-SQLCODE                              00093099
              DISPLAY '* ERROR OPEN CURSOR DESC = ' WS-SQLCODE          00093199
              MOVE 'Y' TO WS-EOF                                        00093399
           END-IF.                                                      00093499
                                                                        00093599
           EXEC SQL                                                     00093699
              OPEN RELAC                                                00093799
           END-EXEC.                                                    00093899
                                                                        00093999
           IF SQLCODE NOT EQUAL ZEROS                                   00094099
              MOVE SQLCODE   TO WS-SQLCODE                              00094199
              DISPLAY '* ERROR OPEN CURSOR RELA = ' WS-SQLCODE          00094299
              MOVE 'Y' TO WS-EOF                                        00094499
           END-IF.                                                      00094599
                                                                        00094699
      *              ---> FECHA EN ENCABEZADO <---                      00094799
           MOVE FUNCTION CURRENT-DATE TO WS-FECHA.                      00094899
           MOVE WS-ANIO               TO WS-IMP-ANIO.                   00094999
           MOVE WS-MES                TO WS-IMP-MES.                    00095099
           MOVE WS-DIA                TO WS-IMP-DIA.                    00095199
                                                                        00095299
      *---->         CARGO TABLA CON PRECIOS DE CAJAS              <----00095399
                                                                        00095499
           SET DESC-INDEX TO 1.                                         00095599
           PERFORM 1100-I-CARGAR-IMPORTES THRU                          00095699
                   1100-F-CARGAR-IMPORTES UNTIL WS-EOF = 'Y'.           00095799
           MOVE 'N' TO WS-EOF.                                          00095899
                                                                        00095999
      *    LIQUIDACION MENSUAL                                          00096099
           IF METODO-LIQUIDACION = 1                                    00096199
              CONTINUE                                                  00096299
              MOVE 'MENSUAL' TO WS-TL                                   00096399
           END-IF.                                                      00096499
                                                                        00097099
      *    LIQUIDACION TRIMESTRAL                                       00097199
           IF METODO-LIQUIDACION = 2                                    00098099
              IF WS-MES EQUAL (3 OR 6 OR 9 OR 12)                       00099099
                 MOVE 3 TO WS-FACTOR-LIQ                                00099199
                 MOVE 'TRIMESTRAL' TO WS-TL                             00099299
              ELSE                                                      00099399
                 MOVE 'Y' TO WS-EOF                                     00099499
              END-IF                                                    00099599
           END-IF.                                                      00099699
                                                                        00099799
      *    LIQUIDACION SEMESTRAL                                        00099899
           IF METODO-LIQUIDACION = 3                                    00099999
              IF WS-MES EQUAL (6 OR 12)                                 00100099
                 MOVE 6 TO WS-FACTOR-LIQ                                00100199
                 MOVE 'SEMESTRAL' TO WS-TL                              00100299
              ELSE                                                      00100399
                 MOVE 'Y' TO WS-EOF                                     00100499
              END-IF                                                    00100599
           END-IF.                                                      00100699
                                                                        00101099
      *    LIQUIDACION ANUAL                                            00102099
           IF METODO-LIQUIDACION = 4                                    00103099
              IF WS-MES EQUAL 12                                        00104099
                 MOVE 12 TO WS-FACTOR-LIQ                               00105099
                 MOVE 'ANUAL' TO WS-TL                                  00105199
              ELSE                                                      00106099
                 MOVE 'Y' TO WS-EOF                                     00107099
              END-IF                                                    00108099
           END-IF.                                                      00109099
                                                                        00109199
      *    SIN LIQUIDACION                                              00109299
           IF METODO-LIQUIDACION = 0                                    00109399
              DISPLAY 'NO SE VA A LIQUIDAR'                             00109499
              MOVE 'Y' TO WS-EOF                                        00109599
           END-IF.                                                      00109699
                                                                        00109799
       1000-F-INICIO. EXIT.                                             00110069
                                                                        00163099
       1100-I-CARGAR-IMPORTES.                                          00163199
                                                                        00163299
           EXEC SQL                                                     00163399
              FETCH DESCR INTO                                          00163499
                 :CD-CS-TIP-CAJA,                                       00163599
                 :CD-CS-ANCHO,                                          00163699
                 :CD-CS-ALTO,                                           00163799
                 :CD-CS-PROFU,                                          00163899
                 :CD-CS-COMI,                                           00163999
                 :CD-CS-SEGURO,                                         00164099
                 :CD-CS-COSTO-LLAVE,                                    00164199
                 :CD-CS-UBICACION-I,                                    00164299
                 :CD-CS-UBICACION-F                                     00164399
           END-EXEC                                                     00164499
                                                                        00164599
           EVALUATE TRUE                                                00165499
           WHEN SQLCODE EQUAL ZERO                                      00165599
              MOVE CD-CS-TIP-CAJA TO TBD-TIPO(DESC-INDEX)               00165699
              MOVE CD-CS-COMI     TO TBD-COMI(DESC-INDEX)               00165799
              SET DESC-INDEX UP BY 1                                    00165899
           WHEN SQLCODE EQUAL +100                                      00165999
              MOVE 'Y'         TO WS-EOF                                00166099
           WHEN OTHER                                                   00166199
              MOVE SQLCODE TO WS-SQLCODE                                00166299
              DISPLAY 'ERROR FETCH CURSOR DESCR ' WS-SQLCODE            00166399
              MOVE 'Y'         TO WS-EOF                                00166499
           END-EVALUATE.                                                00166599
                                                                        00166699
       1100-F-CARGAR-IMPORTES. EXIT.                                    00166799
                                                                        00166899
       2000-I-PROCESO.                                                  00166999
           EXEC SQL                                                     00167099
              FETCH RELAC INTO                                          00167199
                 :CR-CS-TIP-DOC,                                        00167299
                 :CR-CS-NRO-DOC,                                        00167399
                 :CR-CS-NRO-CAJA,                                       00167499
                 :CR-CS-TIP-CUENTA,                                     00167599
                 :CR-CS-NRO-CUENTA,                                     00167699
                 :CR-CS-TIP-DOC-AUT,                                    00167799
                 :CR-CS-NRO-DOC-AUT,                                    00167899
                 :CR-CS-POR-BONIF-COMI,                                 00167999
                 :CR-CS-FEC-ALTA                                        00168099
           END-EXEC                                                     00168199
                                                                        00172599
           EVALUATE TRUE                                                00172899
           WHEN SQLCODE EQUAL ZERO                                      00172999
              ADD 1 TO WS-TOTAL-PROCESADOS                              00173099
                                                                        00173199
              PERFORM 2100-I-BUSCAR-NROCLI THRU                         00173299
                      2100-F-BUSCAR-NROCLI                              00173399
                                                                        00173499
              PERFORM 2200-I-CALCULAR-LIQ THRU                          00173599
                      2200-F-CALCULAR-LIQ                               00173699
           WHEN SQLCODE EQUAL +100                                      00173799
              MOVE 'Y'        TO WS-EOF                                 00173899
                                                                        00173999
              PERFORM 2300-I-ESCRIBIR-ARCHIVOS THRU                     00174099
                      2300-F-ESCRIBIR-ARCHIVOS                          00174199
                                                                        00174299
           WHEN OTHER                                                   00174399
              MOVE SQLCODE TO WS-SQLCODE                                00174499
              DISPLAY 'ERROR FETCH RELACIONES ' WS-SQLCODE              00174599
           END-EVALUATE.                                                00174699
                                                                        00174799
       2000-F-PROCESO. EXIT.                                            00175069
                                                                        00175699
       2100-I-BUSCAR-NROCLI.                                            00175799
                                                                        00175899
           MOVE CR-CS-TIP-DOC TO TIP-DOC                                00175999
           MOVE CR-CS-NRO-DOC TO NRO-DOC                                00176099
           READ MCLIEN INTO REG-CLIENTE KEY IS CLI-KEY                  00176199
           EVALUATE WS-CLI-CODE                                         00176299
           WHEN '00'                                                    00176399
              CONTINUE                                                  00176499
           WHEN OTHER                                                   00176599
              DISPLAY 'ERROR AL BUSCAR NROCLI ' WS-CLI-CODE             00176699
           END-EVALUATE.                                                00176799
                                                                        00176899
       2100-F-BUSCAR-NROCLI. EXIT.                                      00177099
                                                                        00178099
       2200-I-CALCULAR-LIQ.                                             00179099
                                                                        00179199
           PERFORM 2210-I-BUSCAR-PRECIO-CAJA THRU                       00179299
                   2210-F-BUSCAR-PRECIO-CAJA                            00179399
                                                                        00179499
           SET LIQ-INDEX           TO CR-CS-NRO-CAJA.                   00179599
                                                                        00179699
           MOVE CLI-CLI-NRO        TO TBL-NROCLI(LIQ-INDEX).            00179799
           MOVE CR-CS-NRO-CUENTA   TO TBL-NROCUEN(LIQ-INDEX).           00179899
                                                                        00179999
                                                                        00180099
           COMPUTE TBL-IMPORTE(LIQ-INDEX) =                             00180199
                    (WS-PRECIO-CAJA -                                   00180299
                     ((WS-PRECIO-CAJA * CR-CS-POR-BONIF-COMI) / 100)) * 00180399
                        WS-FACTOR-LIQ.                                  00180499
                                                                        00180599
           MOVE CR-CS-TIP-DOC        TO TBL-TIPDOC(LIQ-INDEX).          00180699
           MOVE CR-CS-NRO-DOC        TO TBL-NRODOC(LIQ-INDEX).          00180799
           MOVE CR-CS-NRO-CAJA       TO TBL-NROCAJ(LIQ-INDEX).          00180899
           MOVE TIPO                 TO TBL-TIPCAJ(LIQ-INDEX).          00180999
           MOVE CR-CS-POR-BONIF-COMI TO TBL-BONIF(LIQ-INDEX).           00181099
                                                                        00181199
       2200-F-CALCULAR-LIQ. EXIT.                                       00181299
                                                                        00181399
                                                                        00181499
       2210-I-BUSCAR-PRECIO-CAJA.                                       00182099
                                                                        00182199
           IF CR-CS-NRO-CAJA >= 1 AND CR-CS-NRO-CAJA <= 121             00182299
              MOVE 'SMA' TO TIPO                                        00182399
           END-IF.                                                      00182499
                                                                        00182599
           IF CR-CS-NRO-CAJA >= 122 AND CR-CS-NRO-CAJA <= 221           00182699
              MOVE 'MED' TO TIPO                                        00182799
           END-IF.                                                      00182899
                                                                        00182999
           IF CR-CS-NRO-CAJA >= 222 AND CR-CS-NRO-CAJA <= 321           00183099
              MOVE 'LAR' TO TIPO                                        00183199
           END-IF.                                                      00183299
                                                                        00183399
           IF CR-CS-NRO-CAJA >= 322 AND CR-CS-NRO-CAJA <= 351           00183499
              MOVE 'HIP' TO TIPO                                        00183599
           END-IF.                                                      00183699
                                                                        00183899
           SET DESC-INDEX TO 1.                                         00183999
           SEARCH TBD-DESC                                              00184099
              AT END DISPLAY 'NO ENCUENTRO VALOR CAJA '                 00184199
              WHEN TBD-TIPO(DESC-INDEX) = TIPO                          00184299
                 MOVE TBD-COMI(DESC-INDEX) TO WS-PRECIO-CAJA            00184399
           END-SEARCH.                                                  00184499
