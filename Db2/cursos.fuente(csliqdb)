                    FILE STATUS IS WS-REP-CODE.                         00025099
                                                                        00025199
             SELECT LIQUI ASSIGN DDLIQ                                  00025299
                    FILE STATUS IS WS-LIQ-CODE.                         00025399
                                                                        00025499
       DATA DIVISION.                                                   00025506
       FILE SECTION.                                                    00025606
