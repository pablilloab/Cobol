                    ORGANIZATION IS INDEXED                             00024399
                    ACCESS MODE IS RANDOM                               00024499
                    RECORD KEY IS VIS-KEY                               00024599
                    FILE STATUS IS WS-VIS-CODE.                         00024699
                                                                        00024799
             SELECT REPORTE ASSIGN DDREP                                00024899
                    FILE STATUS IS WS-REP-CODE.                         00024999
