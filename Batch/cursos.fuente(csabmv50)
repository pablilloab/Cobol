             SELECT RELAC ASSIGN DDRELA                                 00023099
                    ORGANIZATION IS INDEXED                             00023199
                    ACCESS MODE IS RANDOM                               00023299
                    RECORD KEY IS REL-KEY                               00023399
                    ALTERNATE KEY IS REL-NRO-CAJA                       00023499
                    FILE STATUS IS WS-REL-CODE.                         00023599
                                                                        00023699
             SELECT NOVVIS ASSIGN DDNOVV                                00023799
                    FILE STATUS IS WS-NVI-CODE.                         00023899
                                                                        00023999
